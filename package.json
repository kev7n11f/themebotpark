{
  "name": "themebotpark",
  "version": "1.0.0",
  "description": "A fullstack app using React and Node.js/Express",
  "homepage": "/",
  "scripts": {
    "client": "cross-env NODE_OPTIONS=--openssl-legacy-provider react-scripts start",
    "server": "nodemon server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider GENERATE_SOURCEMAP=false INLINE_RUNTIME_CHUNK=false react-scripts build",
    "build:analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "start": "cross-env NODE_ENV=production node server.js",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "optimize": "npm run lint:fix && npm run build && npm run test:coverage"
  },
  "dependencies": {
    "@stripe/stripe-js": "^4.8.0",
    "@vercel/analytics": "^1.3.1",
    "@vercel/speed-insights": "^1.0.12",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "framer-motion": "^11.11.11",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.453.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.5",
    "openai": "^5.10.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.28.0",
    "react-scripts": "^5.0.1",
    "stripe": "^18.3.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "vercel": "^47.1.4",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "overrides": {
    "resolve-url-loader": {
      "postcss": "^8.4.47"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "last 3 Chrome versions",
      "last 3 Firefox versions",
      "last 3 Safari versions",
      "last 3 Edge versions"
    ],
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 safari versions",
      "last 2 edge versions"
    ]
  }
}
