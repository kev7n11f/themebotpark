{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" },
    { "src": "public/index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1.js" },
    { "src": "/static/(.*)", "dest": "/build/static/$1" },
    { "src": "/(.*)", "dest": "/public/index.html" }
  ]
}