# Environment Variables for ThemeBotPark

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Stripe Configuration
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here

# React App Environment Variables (must start with REACT_APP_)
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here

# JWT Secret for Authentication
JWT_SECRET=your_jwt_secret_here

# Database Configuration (for future use)
# DATABASE_URL=your_database_url_here

# Weaviate Configuration (for future use)
# WEAVIATE_URL=your_weaviate_url_here
# WEAVIATE_API_KEY=your_weaviate_api_key_here

# PostHog Configuration (for future use)
# POSTHOG_API_KEY=your_posthog_api_key_here

# Stripe Price IDs (replace with your actual price IDs from Stripe Dashboard)
# STRIPE_MONTHLY_PRICE_ID=price_1234567890
# STRIPE_YEARLY_PRICE_ID=price_0987654321
